    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "data=pd.read_csv('test.txt', dtype='string')\n",
    "data['decimal']=data['binary'].apply(lambda x: int(x,2))\n",
    "\n",
    "data['divisible_by_two'] = data['decimal'].mod(2)\n",
    "data['divisible_by_four'] = data['decimal'].mod(4)\n",
    "\n",
    "data"
   ]
